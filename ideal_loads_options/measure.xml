        </choice>
        <choice>
          <value>LimitFlowRateAndCapacity</value>
          <display_name>LimitFlowRateAndCapacity</display_name>
        </choice>
      </choices>
    </argument>
    <argument>
      <name>cooling_limit_type</name>
      <display_name>Cooling Limit Type:</display_name>
      <type>Choice</type>
