      <default_value>NoLimit</default_value>
      <choices>
        <choice>
          <value>NoLimit</value>
          <display_name>NoLimit</display_name>
        </choice>
        <choice>
          <value>LimitFlowRate</value>
          <display_name>LimitFlowRate</display_name>
        </choice>
        <choice>
